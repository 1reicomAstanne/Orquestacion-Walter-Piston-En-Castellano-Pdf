{{#if content }}
  <ul class="media-list">
    {{#each content as |client| }}
      <li class="media">
        <!-- {{#link-to "client.edit" client class="pull-left"}} -->
          <img height="36" width="36" class="img-circle pull-left" src={{client.gravatarURL}}>
        <!-- {{/link-to}} -->
        <div class="media-body">
          <h4 class="media-heading">
            <!-- {{link-to client.contactName "client.edit" client}} -->
          </h4>
          <p>{{client.companyName}}</p>
        </div>
      </li>
    {{/each}}
  </ul>
  <br>
{{else}}
  <p>Nie dodano jeszcze Å¼adnego klienta.</p>
{{/if}}

{{link-to "Dodaj nowego klienta" "app.clients.new" class="btn btn-success" active=null}}
